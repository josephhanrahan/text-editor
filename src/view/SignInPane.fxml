<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="652.0" prefWidth="794.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignInController">
   <children>
      <TextField fx:id="usernameTextField" layoutX="237.0" layoutY="273.0" promptText="Enter Username" />
      <Button fx:id="signInbutton" layoutX="343.0" layoutY="313.0" mnemonicParsing="false" onAction="#signInButtonPressed" text="Sign In" />
      <Button fx:id="signUpButton" layoutX="402.0" layoutY="313.0" mnemonicParsing="false" onAction="#signUpButtonPressed" text="Sign Up" />
      <Label fx:id="errorLabel" layoutX="242.0" layoutY="355.0" prefHeight="90.0" prefWidth="321.0" textAlignment="CENTER" />
      <PasswordField fx:id="passwordTextField" layoutX="402.0" layoutY="273.0" promptText="Enter Password" />
   </children>
</AnchorPane>
